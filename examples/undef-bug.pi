:l Bool.pi

data : Type;
El : data -> Type;

data = ( l : {maybe} ) * 
       case l of {
         maybe -> [data] };


eq : (a : data) -> El a -> El a -> Bool;
eq = \ a x y -> split a with (la,a') -> 
       	          case a' of {
		    maybe -> 'true
		    };


